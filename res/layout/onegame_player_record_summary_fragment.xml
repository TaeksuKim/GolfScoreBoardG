<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            style="?attr/listSectionHeader"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/fragment_player_record_ranking" />

        <RelativeLayout
            android:layout_width="276dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal" >

            <!-- HEAD -->

            <TextView
                android:id="@+id/RankingRightSameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="2dip"
                android:layout_marginTop="10dip"
                android:gravity="center"
                android:text="@string/fragment_player_record_joint" />

            <TextView
                android:id="@+id/RankingRightValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/RankingRightSameValueTextView"
                android:layout_toLeftOf="@+id/RankingRightSameValueTextView"
                android:gravity="center"
                android:text="@string/fragment_player_record_solo" />

            <TextView
                android:id="@+id/RankingRightTitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/RankingRightSameValueTextView"
                android:layout_marginLeft="16dip"
                android:layout_toLeftOf="@+id/RankingRightValueTextView"
                android:text="" />

            <TextView
                android:id="@+id/RankingLeftSameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/RankingRightSameValueTextView"
                android:layout_toLeftOf="@+id/RankingRightTitleTextView"
                android:gravity="center"
                android:text="@string/fragment_player_record_joint" />

            <TextView
                android:id="@+id/RankingLeftValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/RankingLeftSameValueTextView"
                android:layout_toLeftOf="@+id/RankingLeftSameValueTextView"
                android:gravity="center"
                android:text="@string/fragment_player_record_solo" />

            <TextView
                android:id="@+id/RankingLeftTitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/RankingLeftSameValueTextView"
                android:layout_toLeftOf="@+id/RankingLeftValueTextView"
                android:text="" />

            <!-- ROW 1 -->

            <TextView
                android:id="@+id/Ranking2SameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/RankingRightSameValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/RankingRightSameValueTextView"
                android:layout_marginBottom="2dip"
                android:layout_marginTop="2dip"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking2ValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking2SameValueTextView"
                android:layout_alignLeft="@+id/RankingRightValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking2TitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking2SameValueTextView"
                android:layout_alignLeft="@+id/RankingRightTitleTextView"
                android:layout_alignWithParentIfMissing="true"
                android:text="@string/fragment_player_record_ranking_2" />

            <TextView
                android:id="@+id/Ranking1SameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking2SameValueTextView"
                android:layout_alignLeft="@+id/RankingLeftSameValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking1ValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking2SameValueTextView"
                android:layout_alignLeft="@+id/RankingLeftValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking1TitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking2SameValueTextView"
                android:layout_alignLeft="@+id/RankingLeftTitleTextView"
                android:layout_alignWithParentIfMissing="true"
                android:text="@string/fragment_player_record_ranking_1" />

            <!-- ROW 2 -->

            <TextView
                android:id="@+id/Ranking4SameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/Ranking2SameValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/Ranking2SameValueTextView"
                android:layout_marginBottom="2dip"
                android:layout_marginTop="2dip"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking4ValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking4SameValueTextView"
                android:layout_alignLeft="@+id/Ranking2ValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking4TitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking4SameValueTextView"
                android:layout_alignLeft="@+id/Ranking2TitleTextView"
                android:layout_alignWithParentIfMissing="true"
                android:text="@string/fragment_player_record_ranking_4" />

            <TextView
                android:id="@+id/Ranking3SameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking4SameValueTextView"
                android:layout_alignLeft="@+id/Ranking1SameValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking3ValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking4SameValueTextView"
                android:layout_alignLeft="@+id/Ranking1ValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking3TitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking4SameValueTextView"
                android:layout_alignLeft="@+id/Ranking1TitleTextView"
                android:layout_alignWithParentIfMissing="true"
                android:text="@string/fragment_player_record_ranking_3" />

            <!-- ROW 3 -->

            <TextView
                android:id="@+id/Ranking6SameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/Ranking4SameValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/Ranking4SameValueTextView"
                android:layout_marginBottom="2dip"
                android:layout_marginTop="2dip"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking6ValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking6SameValueTextView"
                android:layout_alignLeft="@+id/Ranking4ValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking6TitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking6SameValueTextView"
                android:layout_alignLeft="@+id/Ranking4TitleTextView"
                android:layout_alignWithParentIfMissing="true"
                android:text="@string/fragment_player_record_ranking_6" />

            <TextView
                android:id="@+id/Ranking5SameValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking6SameValueTextView"
                android:layout_alignLeft="@+id/Ranking3SameValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking5ValueTextView"
                android:layout_width="50dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking6SameValueTextView"
                android:layout_alignLeft="@+id/Ranking3ValueTextView"
                android:layout_alignWithParentIfMissing="true"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/Ranking5TitleTextView"
                android:layout_width="30dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/Ranking6SameValueTextView"
                android:layout_alignLeft="@+id/Ranking3TitleTextView"
                android:layout_alignWithParentIfMissing="true"
                android:text="@string/fragment_player_record_ranking_5" />
        </RelativeLayout>

        <TextView
            style="?attr/listSectionHeader"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dip"
            android:text="@string/fragment_player_record_fee" />

        <RelativeLayout
            android:layout_width="276dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal" >

            <TextView
                android:id="@+id/FeeZeroTitleTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
