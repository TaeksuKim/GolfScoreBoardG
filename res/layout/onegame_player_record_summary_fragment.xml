                android:layout_toRightOf="@+id/FeeUnder1000TitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/FeeUnder1500TitleTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/FeeUnder1000TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:gravity="center"
                android:text="@string/fragment_player_record_fee_under1500" />

            <TextView
                android:id="@+id/FeeUnder1500ValueTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/FeeUnder1500TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/FeeUnder1500TitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/FeeUnder2000TitleTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/FeeUnder1500TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/FeeUnder1500ValueTextView"
