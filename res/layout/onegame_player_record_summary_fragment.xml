                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/TripleBogeyTitleTextView"
                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/DoubleBogeyTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/DoubleBogeyValueTextView"
                android:gravity="center"
                android:text="@string/fragment_player_record_triple_bogey" />

            <TextView
                android:id="@+id/TripleBogeyValueTextView"
                android:layout_width="80dip"
