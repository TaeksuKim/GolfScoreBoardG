                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/ParTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/ParValueTextView"
                android:gravity="center"
                android:text="@string/fragment_player_record_bogey" />

            <TextView
                android:id="@+id/BogeyValueTextView"
                android:layout_width="80dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/ParTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/BogeyTitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/DoubleBogeyTitleTextView"
                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/ParTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:gravity="center"
                android:text="@string/fragment_player_record_double_bogey" />

            <TextView
                android:id="@+id/DoubleBogeyValueTextView"
                android:layout_width="80dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/DoubleBogeyTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/DoubleBogeyTitleTextView"
