                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/DoubleBogeyTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:gravity="center"
                android:text="@string/fragment_player_record_quadruple_bogey" />

            <TextView
                android:id="@+id/QuadrupleBogeyValueTextView"
                android:layout_width="80dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/QuadrupleBogeyTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/QuadrupleBogeyTitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/OverQuintupleBogeyTitleTextView"
                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/QuadrupleBogeyTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/QuadrupleBogeyValueTextView"
                android:gravity="center"
                android:text="@string/fragment_player_record_over_quintuple_bogey" />

            <TextView
                android:id="@+id/OverQuintupleBogeyValueTextView"
                android:layout_width="80dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/QuadrupleBogeyTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/OverQuintupleBogeyTitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />
        </RelativeLayout>
    </LinearLayout>

</ScrollView>