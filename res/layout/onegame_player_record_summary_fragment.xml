                android:text="@string/fragment_player_record_fee_under2000" />

            <TextView
                android:id="@+id/FeeUnder2000ValueTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/FeeUnder1500TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/FeeUnder2000TitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/FeeUnder2500TitleTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/FeeUnder1500TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:gravity="center"
                android:text="@string/fragment_player_record_fee_under2500" />

            <TextView
                android:id="@+id/FeeUnder2500ValueTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/FeeUnder2500TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/FeeUnder2500TitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/FeeOver2500TitleTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/FeeUnder2500TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/FeeUnder2500ValueTextView"
                android:gravity="center"
                android:text="@string/fragment_player_record_fee_over2500" />

            <TextView
                android:id="@+id/FeeOver2500ValueTextView"
                android:layout_width="70dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/FeeUnder2500TitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/FeeOver2500TitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />
        </RelativeLayout>

        <TextView
            style="?attr/listSectionHeader"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dip"
            android:text="@string/fragment_player_record_score" />

        <RelativeLayout
            android:layout_width="276dip"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal" >

            <TextView
                android:id="@+id/UnderCondorTitleTextView"
                android:layout_width="60dip"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:gravity="center"
                android:text="@string/fragment_player_record_under_condor" />

            <TextView
                android:id="@+id/UnderCondorValueTextView"
                android:layout_width="80dip"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/UnderCondorTitleTextView"
                android:layout_marginBottom="4dip"
                android:layout_marginTop="4dip"
                android:layout_toRightOf="@+id/UnderCondorTitleTextView"
                android:gravity="center"
                android:textAppearance="?attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/AlbatrossTitleTextView"
